                        @* here *@


                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="math">
                                <div class="row inner text-center">
                                    <div class="col-sm-12">
                                        <button type="button" id="math-btn" class="btn btn-default my-btn">DO MATH MAGIC</button>
                                        <button type="button" id="math-clr" class="btn btn-danger my-btn">CLEAR</button>
                                        <button type="button" id="math-show" class="btn btn-primary my-btn">SHOW CODE</button>
                                    </div>
                                </div>
                                <div class="container-fluid" id="math-content">
                                    <div class="row">
                                        <div class="col-sm-4 hidden-xs">
                                            <div class="row inner">
                                                <label for="inputMath">Enter first number:</label>

                                            </div>
                                            <div class="row inner">
